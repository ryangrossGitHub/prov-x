type Query {
    heartbeat: String
}

type Mutation {
    upsertProv(input: Entity): String
}

input Entity {
    id: String
    wasDerivedFrom: [Entity]
    wasGeneratedBy: Activity
    wasAttributedTo: Agent
}

input Activity {
    id: String
    startedAtTime: String
    endedAtTime: String
}

input Agent {
    id: String
    actedOnBehalfOf: Agent
}